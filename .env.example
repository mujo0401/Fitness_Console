# Flask configuration
SECRET_KEY=your_secret_key_here

# Fitbit API configuration
FITBIT_CLIENT_ID=your_fitbit_client_id
FITBIT_CLIENT_SECRET=your_fitbit_client_secret
FITBIT_REDIRECT_URI=http://localhost:5000/api/auth/callback

# Apple Fitness API configuration
APPLE_FITNESS_CLIENT_ID=your_apple_client_id
APPLE_FITNESS_CLIENT_SECRET=your_apple_client_secret
APPLE_FITNESS_TEAM_ID=your_apple_team_id
APPLE_FITNESS_KEY_ID=your_apple_key_id
